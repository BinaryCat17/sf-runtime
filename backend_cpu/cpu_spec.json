{
  "backend": "cpu",
  "lang": "c11",
  "implementations": {
    "ADD": { "expression": "(va + vb)" },
    "SUB": { "expression": "(va - vb)" },
    "MUL": { "expression": "(va * vb)" },
    "DIV": { "expression": "(va / vb)" },
    "ABS": { "expression": "fabsf(va)" },
    "SIN": { "expression": "sinf(va)" },
    "COS": { "expression": "cosf(va)" },
    "SQRT": { "expression": "sqrtf(va)" },
    "FLOOR": { "expression": "floorf(va)" },
    "CEIL": { "expression": "ceilf(va)" },
    "POW": { "expression": "powf(va, vb)" },
    "ATAN2": { "expression": "atan2f(va, vb)" },
    "MIN": { "expression": "(va < vb ? va : vb)" },
    "MAX": { "expression": "(va > vb ? va : vb)" },
    "FMA": { "expression": "fmaf(va, vb, vc)" },
    "CLAMP": { "expression": "fminf(fmaxf(va, vb), vc)" },
    "STEP": { "expression": "(vb < va ? 0.0f : 1.0f)" },
    "MIX": { "expression": "(va * (1.0f - vc) + vb * vc)" },
    "SELECT": { "expression": "(va ? vb : vc)" },
    "LESS": { "expression": "(va < vb)" },
    "GREATER": { "expression": "(va > vb)" },
    "EQUAL": { "expression": "(va == vb)" },
    "NEQUAL": { "expression": "(va != vb)" },
    "LEQUAL": { "expression": "(va <= vb)" },
    "GEQUAL": { "expression": "(va >= vb)" },
    "AND": { "expression": "(va && vb)" },
    "OR": { "expression": "(va || vb)" },
    "XOR": { "expression": "((int)va != (int)vb)" },
    "NOT": { "expression": "(!va)" }
  }
}
